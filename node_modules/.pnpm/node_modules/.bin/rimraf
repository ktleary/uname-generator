#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/user/projects/uname-generator/node_modules/.pnpm/rimraf@2.6.3/node_modules/rimraf/bin.js/node_modules:/home/user/projects/uname-generator/node_modules/.pnpm/rimraf@2.6.3/node_modules/rimraf/node_modules:/home/user/projects/uname-generator/node_modules/.pnpm/rimraf@2.6.3/node_modules:/home/user/projects/uname-generator/node_modules/.pnpm/node_modules:/home/user/projects/uname-generator/node_modules:/home/user/projects/node_modules:/home/user/node_modules:/home/node_modules:/node_modules:/home/user/projects/uname-generator/node_modules/.pnpm/node_modules/rimraf/bin.js/node_modules:/home/user/projects/uname-generator/node_modules/.pnpm/node_modules/rimraf/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rimraf/bin.js" "$@"
else 
  exec node  "$basedir/../rimraf/bin.js" "$@"
fi
