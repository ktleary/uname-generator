#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/user/projects/uname-generator/node_modules/.pnpm/prettier@2.2.0/node_modules/prettier/bin-prettier.js/node_modules:/home/user/projects/uname-generator/node_modules/.pnpm/prettier@2.2.0/node_modules/prettier/node_modules:/home/user/projects/uname-generator/node_modules/.pnpm/prettier@2.2.0/node_modules:/home/user/projects/uname-generator/node_modules/.pnpm/node_modules:/home/user/projects/uname-generator/node_modules:/home/user/projects/node_modules:/home/user/node_modules:/home/node_modules:/node_modules:/home/user/projects/uname-generator/node_modules/prettier/bin-prettier.js/node_modules:/home/user/projects/uname-generator/node_modules/prettier/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../prettier/bin-prettier.js" "$@"
else 
  exec node  "$basedir/../prettier/bin-prettier.js" "$@"
fi
