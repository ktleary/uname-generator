#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/user/projects/uname-generator/node_modules/.pnpm/eslint-config-prettier@6.15.0_eslint@7.13.0/node_modules/eslint-config-prettier/bin/cli.js/node_modules:/home/user/projects/uname-generator/node_modules/.pnpm/eslint-config-prettier@6.15.0_eslint@7.13.0/node_modules/eslint-config-prettier/bin/node_modules:/home/user/projects/uname-generator/node_modules/.pnpm/eslint-config-prettier@6.15.0_eslint@7.13.0/node_modules/eslint-config-prettier/node_modules:/home/user/projects/uname-generator/node_modules/.pnpm/eslint-config-prettier@6.15.0_eslint@7.13.0/node_modules:/home/user/projects/uname-generator/node_modules/.pnpm/node_modules:/home/user/projects/uname-generator/node_modules:/home/user/projects/node_modules:/home/user/node_modules:/home/node_modules:/node_modules:/home/user/projects/uname-generator/node_modules/eslint-config-prettier/bin/cli.js/node_modules:/home/user/projects/uname-generator/node_modules/eslint-config-prettier/bin/node_modules:/home/user/projects/uname-generator/node_modules/eslint-config-prettier/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../eslint-config-prettier/bin/cli.js" "$@"
else 
  exec node  "$basedir/../eslint-config-prettier/bin/cli.js" "$@"
fi
